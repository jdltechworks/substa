{
  "source": {
    "include": [
    	"./js/react",
    	"README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|dist|bootstrap/index.js|js/components/index.js)"
  },
	"plugins": [
	  "plugins/markdown"
	],
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "./dist/documentation",
    "recurse": true,
    "verbose": true
  },
  "docdash": {
      "static": true,  
      "sort": true
  }
}